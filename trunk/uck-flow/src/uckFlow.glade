                    <child>
                      <widget class="GtkLabel" id="unpackIsoLabel">
                        <property name="visible">True</property>
                        <property name="tooltip" translatable="yes">Once you have created a project, enable this action to unpack the ISO image into its constituents. You need to do this at least once for every image you want to customize.</property>
                        <property name="xalign">0</property>
                        <property name="label" translatable="yes">Unpack the source ISO to perform customization.</property>
                        <property name="wrap">True</property>
                      </widget>
                      <packing>
